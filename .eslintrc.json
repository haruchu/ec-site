{
  "extends": ["next", "prettier", "plugin:storybook/recommended"],
  "rules": {
    "@next/next/no-img-element": "off",
    // import の順番をルール化
    "import/order": [
      "error",
      {
        "alphabetize": {
          "order": "asc"
        }
      }
    ],
    "react/display-name": "off"
  }
}
